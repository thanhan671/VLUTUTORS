<!DOCTYPE html>
<html lang="en">
<head>
    <!--====== Required meta tags ======-->
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <meta name="description" content="" />
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no" />

    <!--====== Title ======-->
    <title>Gia Sư Văn Lang | @ViewData["Title"]</title>

    <!--====== Favicon Icon ======-->
    <link rel="shortcut icon" href="~/images/favicon2.png" type="image/png" />

    <!--====== Slick css ======-->
    <link rel="stylesheet" href="css/slick.css" />

    <!--====== Animate css ======-->
    <link rel="stylesheet" href="css/animate.css" />

    <!--====== Nice Select css ======-->
    <link rel="stylesheet" href="css/nice-select.css" />

    <!--====== Nice Number css ======-->
    <link rel="stylesheet" href="css/jquery.nice-number.min.css" />

    <!--====== Magnific Popup css ======-->
    <link rel="stylesheet" href="css/magnific-popup.css" />

    <!--====== Bootstrap css ======-->
    <link rel="stylesheet" href="css/bootstrap.min.css" />

    <link rel="stylesheet" href="css/font-awesome.min.css" />
    <!--====== Fontawesome css ======-->

    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css"
          integrity="sha512-xh6O/CkQoPOWDdYTDqeRdPCVd1SpvCA9XXcUnZS2FmJNp1coAFzvtCN9BmamE+4aHK8yyUHUSCcJHgXloTyT2A=="
          crossorigin="anonymous"
          referrerpolicy="no-referrer" />

    <!--====== Default css ======-->
    <link rel="stylesheet" href="css/default.css" />

    <!--====== Style css ======-->
    <link rel="stylesheet" href="css/style.css" />

    <!--====== Responsive css ======-->
    <link rel="stylesheet" href="css/responsive.css" />
</head>

<body>
    <!--====== PRELOADER PART START ======-->

    <div class="preloader">
        <div class="loader rubix-cube">
            <div class="layer layer-1"></div>
            <div class="layer layer-2"></div>
            <div class="layer layer-3 color-1"></div>
            <div class="layer layer-4"></div>
            <div class="layer layer-5"></div>
            <div class="layer layer-6"></div>
            <div class="layer layer-7"></div>
            <div class="layer layer-8"></div>
        </div>
    </div>

    <!--====== PRELOADER PART START ======-->
    <!--====== HEADER PART START ======-->

    <header id="header-part">
        <div class="navigation navigation-2 navigation-3 nav-nob">
            <div class="container">
                <div class="row no-gutters">
                    <div class="col-lg-11 col-md-10 col-sm-9 col-9">
                        <nav class="navbar navbar-expand-lg">
                            <a class="navbar-brand" href="index.html">
                                <img class="logo" src="images/logo-vlu.png" alt="Logo" />
                            </a>
                            <button class="navbar-toggler"
                                    type="button"
                                    data-toggle="collapse"
                                    data-target="#navbarSupportedContent"
                                    aria-controls="navbarSupportedContent"
                                    aria-expanded="false"
                                    aria-label="Toggle navigation">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>

                            <div class="collapse navbar-collapse sub-menu-bar"
                                 id="navbarSupportedContent">
                                <ul class="navbar-nav ml-auto">
                                    <li class="nav-item">
                                        <a class="active" href="index.html">Trang Chủ</a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="search.html">Tìm Gia Sư</a>
                                    </li>

                                    <li class="nav-item">
                                        <a href="about.html">Về Chúng Tôi</a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="contact.html">Liên Hệ</a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="becomeatutor.html">Trở Thành Gia Sư</a>
                                    </li>
                                    <li>
                                        <a href="login.html" target="_blank">Trần Vũ Nhật Quỳnh</a>
                                        <ul class="sub-menu">
                                            <li><a href="user-profile.html">Tài Khoản</a></li>
                                            <li><a href="history.html">Lịch Sử Đặt Lịch</a></li>
                                            <li><a href="#">Gia Sư Yêu Thích</a></li>
                                            <li><a href="" id="logout">Đăng Xuất</a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </nav>
                        <!-- nav -->
                    </div>
                </div>
                <!-- row -->
            </div>
            <!-- container -->
        </div>
    </header>

    <!--====== HEADER PART ENDS ======-->

    @RenderBody();

    <!--====== FOOTER PART START ======-->

    <footer id="footer-part">
        <div class="footer-top pt-40 pb-70">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-md-6">
                        <div class="footer-about mt-40">
                            <div class="logo">
                                <a href="#"><img src="images/logo-vlu.png" alt="Logo" /></a>
                            </div>
                            <p>
                                Gia sư Văn Lang nơi kết nối và giúp bạn cải thiện kết quả học
                                tập của bản thân. Bằng cách học mới mẻ, hiện đại gia sư Văn
                                Lang sẽ là người bạn đồng hành thân thiết của các bạn sinh
                                viên!
                            </p>
                            <ul class="mt-20">
                                <li>
                                    <a href="https://www.facebook.com/ctsvvlu">
                                        <i class="fa-brands fa-facebook"></i>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <!-- footer about -->
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6">
                        <div class="footer-link mt-40">
                            <div class="footer-title pb-25">
                                <h6>Về VLUTUTORS</h6>
                            </div>
                            <ul>
                                <li>
                                    <a href="index.html"><i class="fa fa-angle-right"></i>Trang chủ</a>
                                </li>

                                <li>
                                    <a href="about.html"><i class="fa fa-angle-right"></i>Về chúng tôi</a>
                                </li>
                            </ul>
                            <ul>
                                <li>
                                    <a href="search.html"><i class="fa fa-angle-right"></i>Tìm gia sư</a>
                                </li>

                                <li>
                                    <a href="becomeatutor.html"><i class="fa fa-angle-right"></i>Trở thành gia sư</a>
                                </li>
                            </ul>
                        </div>
                        <!-- footer link -->
                    </div>
                    <div class="col-lg-2 col-md-6 col-sm-6">
                        <div class="footer-link support mt-40">
                            <div class="footer-title pb-25">
                                <h6>Hỗ trợ</h6>
                            </div>
                            <ul>
                                <li>
                                    <a href="contact.html"><i class="fa fa-angle-right"></i>Liên hệ</a>
                                </li>
                            </ul>
                        </div>
                        <!-- support -->
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="footer-address mt-40">
                            <div class="footer-title pb-25">
                                <h6>Liên hệ</h6>
                            </div>
                            <ul>
                                <li>
                                    <div class="icon">
                                        <i class="fa fa-home"></i>
                                    </div>
                                    <div class="cont">
                                        <p>
                                            Phòng 1.04, Tòa nhà A, Cơ sở chính, 69/68 Đặng Thùy
                                            Trâm, P. 13, Q. Bình Thạnh, Tp. HCM
                                        </p>
                                    </div>
                                </li>
                                <li>
                                    <div class="icon">
                                        <i class="fa fa-phone"></i>
                                    </div>
                                    <div class="cont">
                                        <p>028 71099218</p>
                                    </div>
                                </li>
                                <li>
                                    <div class="icon">
                                        <i class="fa fa-envelope-o"></i>
                                    </div>
                                    <div class="cont">
                                        <p>t.sv@vanlanguni.edu.vn</p>
                                    </div>
                                </li>
                            </ul>
                        </div>
                        <!-- footer address -->
                    </div>
                </div>
                <!-- row -->
            </div>
            <!-- container -->
        </div>
        <!-- footer top -->
    </footer>

    <!--====== FOOTER PART ENDS ======-->
    <!--====== BACK TO TP PART START ======-->

    <a href="#" class="back-to-top"><i class="fa fa-angle-up"></i></a>

    <!--====== BACK TO TP PART ENDS ======-->
    <!--====== jquery js ======-->
    <script src="js/vendor/modernizr-3.6.0.min.js"></script>
    <script src="js/vendor/jquery-1.12.4.min.js"></script>

    <!--====== Bootstrap js ======-->
    <script src="js/bootstrap.min.js"></script>

    <!--====== Slick js ======-->
    <script src="js/slick.min.js"></script>

    <!--====== Magnific Popup js ======-->
    <script src="js/jquery.magnific-popup.min.js"></script>

    <!--====== Counter Up js ======-->
    <script src="js/waypoints.min.js"></script>
    <script src="js/jquery.counterup.min.js"></script>

    <!--====== Nice Select js ======-->
    <script src="js/jquery.nice-select.min.js"></script>

    <!--====== Nice Number js ======-->
    <script src="js/jquery.nice-number.min.js"></script>

    <!--====== Count Down js ======-->
    <script src="js/jquery.countdown.min.js"></script>

    <!--====== Validator js ======-->
    <script src="js/validator.min.js"></script>

    <!--====== Ajax Contact js ======-->
    <script src="js/ajax-contact.js"></script>

    <!--====== Main js ======-->
    <script src="js/main.js"></script>

    <!--====== Map js ======-->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDC3Ip9iVC0nIxC6V14CKLQ1HZNF_65qEQ"></script>
    <script src="js/map-script.js"></script>
</body>
</html>
