                                <h6>Hỗ trợ</h6>
                            </div>
                            <ul>
                                <li>
                                    <a href="contact.html"><i class="fa fa-angle-right"></i>Liên hệ</a>
                                </li>
                            </ul>
                        </div>
                        <!-- support -->
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="footer-address mt-40">
                            <div class="footer-title pb-25">
                                <h6>Liên hệ</h6>
                            </div>
                            <ul>
                                <li>
                                    <div class="icon">
                                        <i class="fa fa-home"></i>
                                    </div>
                                    <div class="cont">
                                        <p>
                                            Phòng 1.04, Tòa nhà A, Cơ sở chính, 69/68 Đặng Thùy Trâm, P. 13, Q. Bình Thạnh, Tp. HCM
                                        </p>
                                    </div>
                                </li>
                                <li>
                                    <div class="icon">
                                        <i class="fa fa-phone"></i>
                                    </div>
                                    <div class="cont">
                                        <p>028 71099218</p>
                                    </div>
                                </li>
                                <li>
                                    <div class="icon">
                                        <i class="fa fa-envelope-o"></i>
                                    </div>
                                    <div class="cont">
                                        <p>t.sv@vanlanguni.edu.vn</p>
                                    </div>
                                </li>
                            </ul>
                        </div>
                        <!-- footer address -->
                    </div>
                </div>
                <!-- row -->
            </div>
            <!-- container -->
        </div>
